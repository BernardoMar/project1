<%= form_for @menu do |f| %>

  <%=f.label :item%>
  <%=f.text_field :item%>

  <%=f.label :description%>
  <%=f.text_field :description%>

  <%=f.label :category%>
  <%=f.text_field :category%>

  <%=f.label :price%>
  <%=f.number_field :price%>

  <%= f.label :image, "Meal Photo"%>
  <%= f.url_field :image %>

  <%= f.label :venue_id %>
  <%= f.select :venue_id, @current_user.venues.pluck(:name, :id), :include_blank => true  %>


  <%= f.submit %>
<% end %>
