<%= form_for @venue do |f| %>

  <%= f.label :name%>
  <%= f.text_field :name, :required => true  %>

  <%= f.label :address%>
  <%= f.text_field :address %>

  <%= f.label :suburb%>
  <%= f.text_field :suburb %>

  <%= f.label :image %>
  <%= f.url_field :image %>

  <%= f.label :logo %>
  <%= f.url_field :logo %>

  <%= f.label :phone%>
  <%= f.number_field :phone%>

  <%= f.label :website%>
  <%= f.url_field :website %>

  <%= f.label :insta, "Instagram Account"%>
  <%= f.url_field :insta %>

  <%= f.submit %>

<% end %>
