<%= form_for @venue do |f| %>
  <div class="form-group">
    <%= f.label :name%>
    <%= f.text_field :name, :required => true, :class => 'form-control'  %>
  </div>
  <div class="form-group">
    <%= f.label :address%>
    <%= f.text_field :address, :required => true, :class => 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :suburb%>
    <%= f.text_field :suburb, :required => true, :class => 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :image %>
    <%= f.url_field :image, :required => true, :class => 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :logo %>
    <%= f.url_field :logo, :required => true, :class => 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :phone%>
    <%= f.number_field :phone, :required => true, :class => 'form-control'%>
  </div>
  <div class="form-group">
    <%= f.label :website%>
    <%= f.url_field :website, :required => true, :class => 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :insta, "Instagram Account"%>
    <%= f.url_field :insta, :class => 'form-control' %>
  </div>
  <div class="contain_btn">
    <%= f.submit "Submit", :class => "btn btn-info"  %>
  </div>
<% end %>
